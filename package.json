{
  "name": "set",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run-script watch",
    "prebuild": "npm run-script clean",
    "build": "npm run-script build:html && npm run-script build:js && npm run-script build:css",
    "clean": "rimraf dist/*",
    "build:html": "cp app/index.html dist/index.html",
    "build:js": "browserify app/scripts/index.js -o dist/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "build:css": "node-sass app/scss/main.scss dist/main.css",
    "watch": "parallelshell 'npm run-script watch:html' 'npm run-script watch:js' 'npm run-script watch:scss' 'npm run-script serve' 'npm run livereload'",
    "watch:html": "watch-run npm run-script build:html -p 'app/index.html'",
    "watch:js": "watch-run npm run-script build:js -p 'app/scripts'",
    "watch:scss": "watch-run npm run-script build:css -p 'app/scss'",
    "serve": "http-server -p 9090 dist/ -s",
    "livereload": "live-reload --port 9091 dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "NicerHugs",
  "license": "MIT",
  "dependencies": {
    "alt": "^0.17.9",
    "history": "^1.13.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "http-server": "^0.8.5",
    "live-reload": "^1.1.0",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.4.4",
    "watch": "^0.16.0",
    "watch-run": "^1.2.3"
  }
}
